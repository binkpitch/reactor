{
  "name": "reactor",
  "version": "1.4.0",
  "private": true,
  "homepage": "https://binkpitch.github.io/reactor",
  "dependencies": {
    "axios": "^0.17.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-redux": "^5.0.0-alpha.8",
    "redux": "^3.7.2",
    "redux-form": "^7.0.3",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.75.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^4.2.2",
    "@commitlint/config-angular": "^4.2.1",
    "@storybook/addon-actions": "^3.2.13",
    "@storybook/addon-centered": "^3.2.13",
    "@storybook/addon-links": "^3.2.13",
    "@storybook/addon-storyshots": "^3.2.13",
    "@storybook/react": "^3.2.13",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "pre-commit": "^1.2.2",
    "react-scripts": "1.0.15",
    "react-test-renderer": "^16.0.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-immutable-state-invariant": "^2.0.0",
    "standard": "^10.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "test": "CI=true react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "./node_modules/.bin/standard",
    "lint-fix": "./node_modules/.bin/standard --fix",
    "precommit-msg": "echo '\nPre-commit -> Linting with Standard.js -> Running test with Jest \nUse \"--no-verify\" to force commit\n' && exit 0",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "test"
  ],
  "standard": {
    "env": [
      "jest"
    ]
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
